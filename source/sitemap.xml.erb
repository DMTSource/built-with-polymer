---
layout: false
directory_index: false
---
<% site_url = "http://www.builtwithpolymer.org" %>
<% pages = sitemap.resources.find_all{|p| p.source_file.match(/\.html/) && !p.source_file.match(/(bower_components|components|submit-failure|submit-success)/) } %>
<% posts = blog.articles %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |p| %>
  <url>
    <loc><%= URI.join(site_url, p.destination_path.gsub('index.html','')) %></loc>
    <priority>0.7</priority>
  </url>
<% end %>
<% posts.each do |p| %>
  <url>
    <loc><%= URI.join(site_url, p.url) %></loc>
    <priority>0.7</priority>
  </url>
<% end %>
</urlset>